{
    "name": "volodymyr-klymniuk/dto-mapper",
    "type": "library",
    "description": "Data mapper library, based on file template generators and Closure binding.",
    "license": "MIT",
    "authors": [
        {
            "name": "Volodymyr Klymniuk",
            "email": "Volodymyr.Klymniuk@gmail.com"
        }
    ],
    "homepage": "https://github.com/volodymyr-klymniuk/dto-mapper",
    "require": {
        "php": "^7.4",
        "ocramius/generated-hydrator": "4.1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "DataMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "./vendor/bin/phpunit -c tests/phpunit.xml tests/"
    },
    "keywords": [
        "serializer",
        "object conversion",
        "array conversion",
        "performance",
        "hydrator",
        "dto-mapper",
        "mapper",
        "php"
    ]
}
